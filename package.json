{
  "name": "user-service",
  "version": "1.0.0",
  "description": "User service for Trade-Lynk using Bun.js, Hono, MongoDB, and Redis",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --bundle --target=bun",
    "compile": "bun build src/index.ts --compile --target=bun",
    "test": "bun test"
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "hono": "^4.5.0",
    "ioredis": "^5.6.1",
    "mock-aws-s3": "^4.0.2",
    "mongoose": "^8.15.0",
    "mongoose-lean-virtuals": "^2.0.0",
    "nock": "^14.0.4",
    "otpauth": "^9.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.25",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}